<h2 align="center">Edit in devise <%= resource_name.to_s.humanize %></h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
   <!--<%= devise_error_messages! %>-->
 <div class="tb">
    <div class="form-group" >
      <table align="center">
        <tr>
          <td><%= f.label :image, "browse photo",:class =>"form-control" %></td>
          <td><%=f.file_field :image %></td>
        </tr>
        <tr>
          <td><%= f.label "first name" %></td>
          <td><%= f.text_field :firstname, :required=>true, :class =>"form-control", :placeholder =>"first name"%></td>
          <td class="error"><%=@user.errors[:firstname].join(",") %></td>
          <td></td>
        </tr>
        <td></td>
        <tr>
          <td><%= f.label "middlename" %></td>
          <td><%= f.text_field :middlename, :class =>"form-control", :placeholder =>"middle name" %></td>
        </tr>
          <td></td>
        <tr>
          <td><%= f.label "last name" %></td>
          <td><%= f.text_field :lastname, :required=>true,:class =>"form-control", :placeholder =>"last name"%></td>
          <td class="error"><%= @user.errors[:lastname].join(",")  %></td>
        </tr>
          <td></td>
        <tr>
          <td> <%= f.label "email id" %></td>
          <td> <%= f.text_field :email ,:pattern => '[^@]+@[^@]+\.[a-zA-Z]{2,6}',:required=>true,:class =>"form-control", :placeholder =>"email id"%></td>
          <td class="error"> <%= @user.errors[:email].join(",")%></td>
        </tr>
          <td></td>
        <tr>
          <td><%= f.label "login id" %></td>
          <td><%= f.text_field :loginid, :required=>true,:class =>"form-control", :placeholder =>"login id"%></td>
          <td class="error"> <%= @user.errors[:loginid].join(",")%></td>
        </tr>
          <td></td>
          <tr>
          <td>  <%= f.label "current password" %></td>
          <td> <%= f.password_field :current_password, :class =>"form-control", :placeholder =>"current password"%></td>
          <td class="error"> <%= @user.errors[:current_password].join(",")%></td>
        </tr>
        <tr>
          <td>  <%= f.label "new password" %></td>
          <td> <%= f.password_field :password, :class =>"form-control", :placeholder =>"password"%></td>
          <td class="error"> <%= @user.errors[:password_confirmation].join(",")%></td>
        </tr>
        <tr>
          <td>  <%= f.label "confirm password"%></td>
          <td> <%= f.password_field :password_confirmation, :class =>"form-control", :placeholder =>"confirm password"%></td>
        </tr>

          <td></td>
        <tr>
          <td><%= f.label "employee id" %></td>
          <td> <%= f.text_field :empid, :required=>true,:class =>"form-control", :placeholder =>"employee id"%></td>
          <td class="error"> <%= @user.errors[:empid].join(",")%></td>
        </tr>
          <td></td>
        <tr>
          <td>   <%= f.label "Date of birth"%></td>
          <td> <%=f.date_select :dob, order: [ :month, :day, :year],:start_year => 1950, :end_year => 1995%></td>
        </tr>
          <td></td>
        <tr>
          <td><%=f.label "Time Zone"%></td>
          <td> <%=f.time_zone_select("timezone", nil, default: "Pacific Time (US & Canada)", include_blank: false)%></td>
        </tr>
          <td></td>
        <tr>
          <td><%= f.label "Date Of Joing"%></td>
          <td> <%= f.date_select("doj", order: [ :month, :day, :year], :start_year => 1990, :end_year => 2020, selected: Date.today)%></td>
        </tr> 
          <td></td>
        <tr>
          <td><%= f.label "Gender" %></td>
          <td> <%= f.select :gender, ["male","female","others"]%></td>
        </tr>
          <td></td>
        <tr>
          <td><%= f.label "designation" %></td> 
          <td> <%= f.select(:designation_id ,@designations=Designation.all.map{|d|[d.name,d.id]}) %></td>
        </tr>
          <td></td>
        <tr>
          <td><%= f.label "Education"%><br></td> 
          <td> <%= f.text_area :education,:class =>"form-control",:placeholder =>"education"%></td>
        </tr>
          <td></td>
        <tr>
          <td><%= f.label "Comments"%></td> 
          <td>  <%= f.text_area :comments,:class =>"form-control",:placeholder =>"comments"%></td>
        </tr>
          <td></td>
        <tr>
          <td><%= f.label "work phone" %>&nbsp:</td> 
          <td><%= f.text_field :phone, :class =>"form-control", :placeholder =>"enter phone number"%></td>
          <td class="error"><%=@user.errors[:phone].join(",")%></td>
        </tr>
        <tr>
          <div class="actions">
            <td><%= f.submit "Update" %></td>
          </div>
        </tr>
      </table>
    </div>
  </div>
<% end %>




